{
    "name": "silverstripe/recipe-reporting-tools",
    "type": "silverstripe-recipe",
    "description": "Add extra CMS reporting tools to your SilverStripe project",
    "homepage": "https://www.silverstripe.org",
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/recipe-plugin": "^1",
        "silverstripe/recipe-cms": "4.8.0-rc1",
        "silverstripe/externallinks": "2.1.1@stable",
        "silverstripe/securityreport": "2.3.0-rc1",
        "silverstripe/sitewidecontent-report": "3.1.0-rc1",
        "bringyourownideas/silverstripe-maintenance": "2.3.1@stable",
        "bringyourownideas/silverstripe-composer-update-checker": "2.0.3@stable",
        "bringyourownideas/silverstripe-composer-security-checker": "2.0.4@stable"
    },
    "require-dev": {
        "sminnee/phpunit": "^5.7"
    },
    "extra": {
        "project-files": []
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}